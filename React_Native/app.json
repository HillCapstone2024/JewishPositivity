{
  "expo": {
    "name": "Jewish_Positivity",
    "slug": "React_Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jewishpositivity.jewishpositivity",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jewishpositivity.jewishpositivity"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },

  "plugins": [
    [
    "onesignal-expo-plugin",
    {
      "mode": "development"
    }
  ]
],
"extra": {
  "oneSignalAppId": "ec16a462-d3d0-4fa3-85cd-d4bdaa8e98a8"
}
  }
}
